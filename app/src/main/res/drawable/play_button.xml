<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Outer Stroke -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#5b400d" /> <!-- Outer stroke color -->
            <corners android:radius="25dp"/>
            <stroke android:width="6dp" android:color="#fee9a5"/>
        </shape>
    </item>

    <!-- Inner Stroke -->
    <item android:left="3dp" android:right="3dp" android:top="3dp" android:bottom="3dp">
        <shape android:shape="rectangle">
            <solid android:color="#fffce8"/> <!-- Inner shape color -->
            <corners android:radius="22dp"/>
            <stroke android:width="2dp" android:color="#5a400c"/> <!-- Inner stroke -->
        </shape>
    </item>

</layer-list>
